@model IEnumerable<Medicine.WEB.Models.DoctorView>

@{
    ViewBag.Title = "GetListOfDoctors";
}

<h2>GetListOfDoctors</h2>

<p>
    @Html.ActionLink("Create New", "RegisterDoctor")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Surname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Qualification)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfBirth)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Surname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Qualification)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfBirth)
        </td>
        <td>
            @Html.ActionLink("Edit", "UpdateDoctor", new { id=item.Id }) |
            @Html.ActionLink("Details", "GetListOfPatients", new { id=item.Id }) |
            @Html.ActionLink("Delete", "DeleteDoctorView", new { id=item.Id })
        </td>
    </tr>
}

</table>
